
public class ExoOperateur {

	public static void main(String[] args) {
		
		////  noteJava = 14                  coefJava = 3 
	///	      noteHtml = 14                  coefHtml = 2 
	////      noteJavascript = 9            coefJava = 3 
	////      noteCss = 14                  coefcss = 1 
		//		moyenne  moyenne Sans les coef 
		//		MoyenneGenerale : moyenne avec les coef 
		
		
		int notejava = 14;
		int notehtml = 14;
		int notejavasc = 9;
		int notecss = 14;
		int coefjava =3;
		int coefhtml = 2;
		int coefjavasc = 3 ;
		int coefcss =1;
		double moyennesanscoef = (notejava +notehtml +notejavasc+notecss)/4;
		double moyenneaveccoef = ((notejava *coefjava)+(notehtml *coefhtml)*(notejavasc * coefjavasc)+(notecss * coefcss))/(coefjava+coefhtml+coefjavasc+coefcss);
		boolean moyenne = moyennesanscoef >= 10;
		boolean moyenneGenerale = moyenneaveccoef >= 10;
		  
		
		if (moyennesanscoef || moyenneaveccoef >= 10) {
			System.out.println("Vous avez la moyenne");
		} else if (moyennesanscoef || moyenneaveccoef < 10) {
			System.out.println("Vous n'avez pas eu la moyenne");
			
			
			
			
		}
		 
		
		
		
		
		
		
			
			
		
		
		
		
		
		

	}

}
