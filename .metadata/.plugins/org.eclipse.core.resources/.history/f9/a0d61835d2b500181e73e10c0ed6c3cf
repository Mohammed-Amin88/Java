
public class ExoConditionIfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		String connexion = "conecter";
		if (connexion == "conecter") {
			System.out.println( "conecter");
		}
		
		else   {
			System.out.println("non conecter");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
