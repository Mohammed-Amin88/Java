
public class premiereclass {


	public static void main(String[] args) {
		//Afficher une chaine de caractères
		System.out.println("Bonjour");    
		System.out.println("Bonsoir");    
		System.out.println("erreur");
		//afficher un boolean
		System.out.println(true);
		// Afficher plusieurs données
		System.out.println("Bonjour"+"monsieur");
		System.out.println("Bonjour"+ "monsieur" +124);
		
		
		
		System.out.print("Bonjour");
		
		
		//System.out.prinln("bonjour")================System.out.print("bonjour");System.out.printl();
		System.out.println();
		System.out.print("12*12=" +(12*12));
		
	
		
		

		
		
}
}