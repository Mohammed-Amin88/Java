
public class Typesvarriables {

	public static void main(String[] args) {
	   int entier = 14;
	   System.out.println("la variable entier =" + entier);
	    // System.out.println("Bonjour")=========== System.out.print("Bonjour");System.out.println();
	   System.out.println();
	   System.out.print("La variable entier1 * la variable entier2 =" +12*12);
	   
	   
	   //entier1
	   int prix =10 ;
	   
	   //entier2
	   int tva = 20 ;
	            
		System.out.println( "prix * tva =" +(prix * tva));
		
	   //Declaration et effectation
		//Type nomvariable = valeur;
		int monInt =12;
		double monDouble = 15.;
		float monFloat = 12.6f;
		char monChar ='a';
		boolean monBoolean = false;
		String maChain ="chaine";
		System.out.println("Declaration + effectation :");
		System.out.println("monInt =" +monInt);
		System.out.println("monDouble =" + monDouble);
		System.out.println("monFloat =" + monFloat);
		System.out.println("monChar =" + monChar);
		System.out.println("monBoolean =" + monBoolean);
		
		
		//Affectation
		//nomVariabele = valeur ;
		maChaine = "nouvelle chaine" ;
		monChar ='z';
		monBoolean = true;
		monDouble =120.12;
		monInt = 1500 ;
		monFloat = 155.26f;
		System.out.println("Affectation :");
		System.out.println("monInt = "+ monInt);
		System.out.println("monDouble =" + monDouble);
		
				
	
				
				
		
		
		
				
		
		
				
	

		
	   
	   
	   
	   
	   
	   
	   
       
	}
	
	
	

}
